    "eslint-plugin-qunit": "^8.1.2",
    "lint-to-the-future": "^2.6.3",
    "lint-to-the-future-ember-template": "^3.1.0",
    "lint-to-the-future-eslint": "^3.1.0",
    "lint-to-the-future-stylelint": "^2.1.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.3.3",
